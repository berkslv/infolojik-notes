    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select e.FirstName, e.LastName, count(o.OrderID) as OrderCount
from orders o  
left join employees e 
on o.EmployeeID = e.EmployeeID
group by e.FirstName, e.LastName</CommandText>
      </Query>
